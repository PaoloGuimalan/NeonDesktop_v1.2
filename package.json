{
  "name": "neon-pos",
  "version": "0.5.0",
  "license": "MIT",
  "main": "main/index.js",
  "author": {
    "name": "Maximilian St√ºmpfl"
  },
  "description": "Commercial version of NeonPOS",
  "keywords": [
    "vite",
    "react",
    "electron",
    "typescript"
  ],
  "repository": {
    "url": "https://github.com/lTimeless/vite-reactts-electron-starter"
  },
  "productName": "vite-reactts-electron-starter",
  "scripts": {
    "dev": "concurrently \"yarn dev:vite\" \" yarn dev:electron\"",
    "dev:vite": "vite",
    "dev:electron": "npm run build:electron && electron .",
    "build": "npm run build:vite && npm run build:electron",
    "build:vite": "vite build",
    "build:electron": "tsc -p electron",
    "dist": "npm run build && electron-builder",
    "pack": "npm run build && electron-builder --dir",
    "clean": "rimraf dist main src/out",
    "type-check": "tsc",
    "lint": "eslint . --ext js,jsx,ts,tsx",
    "lint:fix": "eslint . --ext js,jsx,ts,tsx --fix"
  },
  "dependencies": {
    "@electron/remote": "^2.1.2",
    "@material-ui/core": "^4.12.4",
    "@material-ui/icons": "^4.11.3",
    "@reduxjs/toolkit": "^2.2.6",
    "@testing-library/jest-dom": "^6.4.8",
    "@testing-library/react": "^16.0.0",
    "@testing-library/user-event": "^14.5.2",
    "@vitejs/plugin-react": "^1.2.0",
    "axios": "^1.7.2",
    "bcrypt": "^5.1.1",
    "bcryptjs-react": "^2.4.6",
    "body-parser": "^1.20.2",
    "chart.js": "^4.4.3",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "electron-is-dev": "2.0.0",
    "express": "^4.19.2",
    "fetch-installed-software": "^0.0.7",
    "find-process": "^1.4.7",
    "framer-motion": "^11.2.10",
    "jsonwebtoken": "^9.0.2",
    "jwt-decode": "^4.0.0",
    "jwt-encode": "^1.0.1",
    "locationiq": "^0.0.3",
    "loudness": "^0.4.2",
    "music-metadata": "^10.0.0",
    "nodemon": "^3.1.4",
    "os-utils": "^0.0.14",
    "platform-folders": "^0.6.0",
    "react": "^18.3.1",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "^18.3.1",
    "react-helmet": "^6.1.0",
    "react-icons": "^5.2.1",
    "react-redux": "^9.1.2",
    "react-router-dom": "^6.25.1",
    "react-scripts": "^5.0.1",
    "read-dir-and-stat": "^1.0.8",
    "url": "^0.11.3",
    "vite": "^2.8.6",
    "wait-on": "^7.2.0",
    "web-vitals": "^4.2.2"
  },
  "devDependencies": {
    "@types/node": "^17.0.22",
    "@types/react": "^17.0.41",
    "@types/react-dom": "^17.0.14",
    "@typescript-eslint/eslint-plugin": "5.16.0",
    "@typescript-eslint/parser": "5.16.0",
    "autoprefixer": "^10.4.4",
    "concurrently": "^8.2.2",
    "electron": "^17.1.2",
    "electron-builder": "^22.14.13",
    "eslint": "8.11.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-react": "^7.29.4",
    "postcss": "^8.4.12",
    "prettier": "2.6.0",
    "tailwindcss": "^3.0.23",
    "typescript": "^4.6.2"
  },
  "build": {
    "asar": true,
    "files": [
      "main",
      "src/out"
    ],
    "directories": {
      "buildResources": "resources"
    }
  }
}
